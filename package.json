{
  "name": "aave-v2-payloads",
  "description": "An opionated template for creating aave proposal payloads.",
  "author": "Yonatan Hayun <johnhayun@chaoslabs.xyz>",
  "version": "0.0.1",
  "license": "AGPL-3.0-only",
  "files": [
    "src/**/*.sol"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
