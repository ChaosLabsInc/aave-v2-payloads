  "homepage": "https://github.com/ChaosLabsInc/aave-v2-payloads#readme",
  "devDependencies": {
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.1",
    "prettier-plugin-solidity": "^1.0.0-beta.19"
  }
}
